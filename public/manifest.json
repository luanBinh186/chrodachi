{
    "manifest_version": 3,
    "name": "Chrodachi",
    "description": "How long have I been on google?",
    "version": "1.0",
    "icons": {
        "19": "assets/browser-action/action-19.png",
        "38": "assets/browser-action/action-38.png",
        "76": "assets/browser-action/browser-action-76.png",
        "152": "assets/browser-action/browser-action-152.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "service-worker/service-worker.js",
        "type": "module"
    },
    "permissions": [
        "storage"
    ]
}